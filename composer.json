{
    "name": "atlas/skeleton",
    "type": "library",
    "description": "Command-line interface to generate Atlas skeleton classes.",
    "keywords": [ "sql", "data", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas.Skeleton Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Skeleton/contributors"
        }
    ],
    "require": {
        "php": "^8.0",
        "atlas/info": "2.x-dev",
        "psr/log": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Skeleton\\": "src/"
        }
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\Skeleton\\": "tests/"
        }
    },
    "bin": [
        "bin/atlas-skeleton.php"
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "stan": "./vendor/bin/phpstan analyze -c phpstan.neon src"
    },
    "minimum-stability": "dev"
}
