{
    "name": "atlas/cli",
    "type": "library",
    "description": "Command-line interface for Atlas.",
    "keywords": [ "sql", "data", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Cli",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas.Cli Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Cli/contributors"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "atlas/info": "1.x@dev",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Cli\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "pds/skeleton": "~1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\Cli\\": "tests/"
        }
    },
    "bin":["bin/atlas-skeleton.php"],
    "minimum-stability": "dev"
}
